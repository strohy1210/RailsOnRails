<div class = "results text-center col-md-8 col-md-offset-2">
  <div class ="results-title" >
<h1> You searched for <%= @commute_dest.query %>. <h1>
  <h2> Here are all the apartments within a 30 min commute of that address with no transfers!</h2>
</div>
<% if @apartments.size > 0 %>
<% @apartments.each_with_index do |apt, i| %>
  <div class="listing center-block text-center ">
  <%= link_to(image_tag(apt.photo_url, class: "img-rounded img-responsive"), apt.link)%><br>
  <h4>Price: <%= apt.price %> |
  Neighborhood: <%= apt.neighborhood %> </h4>
</div>
<%end%>
<%end%>
